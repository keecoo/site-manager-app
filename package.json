    "react": "16.4.1",
    "react-apollo": "^2.1.9",
    "react-native": "0.55.4",
    "react-native-auth0": "^1.3.0",
    "react-native-image-picker": "^0.26.10",
    "react-native-ionicons": "^0.2.2",
    "react-navigation": "2.6.1",
    "react-redux": "^5.0.7",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
